{
  "presets": ["es2015", "flow", "react", "stage-1"],
  "plugins": ["emotion", "transform-class-properties"],
  "env": {
    "development": {
      "presets": ["es2015", "flow", "react", "stage-1"],
      "plugins": ["emotion", "transform-class-properties", "inline-svg"]
    },
    "production": {
      "plugins": [
        "emotion",
        "transform-flow-strip-types",
        "transform-class-properties",
        "inline-svg"
      ]
    }
  }
}
